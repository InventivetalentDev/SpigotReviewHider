{
  "name": "SpigotMC Review Hider",
  "short_name": "ReviewHider",
  "version": "1.0.1",
  "description": "Gets rid of the review section & ratings on SpigotMC.org!",
  "author": "Haylee Sch√§fer (inventivetalent)",
  "manifest_version": 2,
  "icons": {
    "64": "img/ReviewHider-x64.png",
    "128": "img/ReviewHider-x128.png",
    "256": "img/ReviewHider-x256.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.spigotmc.org/resources/*"
      ],
      "js": [
        "/contentScript.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "options_page": "options.html"
}